[texture] organic1
  upsample "BILINEAR"
  downsample "BILINEAR"
  path "data/textures/organic.jpg"

[pipeline] solar
  frag "data/shaders/sol.frag.glsl"
  vert "data/shaders/default.vert.glsl"
  uniforms ["transform", "organic", "time", "sunColor", "raduis"]

[pipeline] sprite 
  frag "data/shaders/sprite.frag.glsl"
  vert "data/shaders/default.vert.glsl"
  uniforms ["transform", "atlas"]

[texture] spritesheet 
  upsample "NEAREST"
  downsample "NEAREST"
  path "data/textures/spritesheet.png"